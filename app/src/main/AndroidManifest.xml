<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.trafimau_app">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_knight"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_knight_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">

        <activity
            android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".WelcomePageStart">
        </activity>

        <activity android:name=".AppDescription" />
        <activity android:name=".ThemePicker" />
        <activity android:name=".LayoutPicker" />

        <activity
            android:name=".LauncherActivity"
            android:label="Launcher Activity"
            android:parentActivityName=".MainActivity">
        </activity>

        <activity
            android:name=".ListActivity"
            android:label="List Activity"
            android:parentActivityName=".MainActivity">
        </activity>

        <meta-data
            android:name="io.fabric.ApiKey"
            android:value="8aeba7c3ec63adc1141616f33036329f545d13e4" />

    </application>

</manifest>